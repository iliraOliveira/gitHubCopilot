{
  "animations": [
    {
      "id": "rouletteSpin",
      "type": "rotation",
      "duration": 5000,
      "easing": "cubic-bezier(0.33, 1, 0.68, 1)",
      "keyframes": [
        {
          "offset": 0,
          "transform": "rotate(0deg)"
        },
        {
          "offset": 1,
          "transform": "rotate(360deg)"
        }
      ]
    },
    {
      "id": "ballBounce",
      "type": "bounce",
      "duration": 1000,
      "easing": "ease-in-out",
      "keyframes": [
        {
          "offset": 0,
          "transform": "translateY(0)"
        },
        {
          "offset": 0.5,
          "transform": "translateY(-20px)"
        },
        {
          "offset": 1,
          "transform": "translateY(0)"
        }
      ]
    }
  ]
}